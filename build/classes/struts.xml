<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
  <package name = "pdo"  extends = "struts-default" namespace = "/">
    <action name = "actionAddPdo" class = "action.PDO" method = "addPdo">
      <result>/addSuccess.jsp</result>
      <result name = "error">/addFail.jsp</result>
    </action>
    <action name = "actionQueryPdo" class = "action.PDO" method = "queryPdo">
      <result>/addSuccess.jsp</result>
      <result name = "error">/addFail.jsp</result>
    </action>
  </package>
</struts>